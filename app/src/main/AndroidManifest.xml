<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.hello.buildspec">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_gradapp"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher_gradapp"
        android:supportsRtl="true"
        android:theme="@style/RealAppTheme">
        <activity android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".ViewPager_Main"/>
        <activity
            android:name=".GetCalendar"
            android:theme="@android:style/Theme.Dialog"
            />


    </application>

</manifest>